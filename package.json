{
  "name": "octofarm",
  "version": "1.1.6-dev7",
  "description": "OctoFarm is a easy to setup and install web interface that unifies your Octoprint instances for easy monitoring of all your printers. OctoFarm allows you to connect to multiple octoprint instances on your network allowing creating a farm management system. You can control, manage, upload files to any of your instances without ever leaving the tab it's open in.",
  "main": "app.js",
  "scripts": {
    "prestart": "npm install --production",
    "start": "pm2 flush && pm2 start app.js --name OctoFarm -e './logs/pm2.error.log' --time",
    "stop": "pm2 stop OctoFarm",
    "startup": "pm2 startup",
    "saveStartup": "pm2 save",
    "dev": "nodemon ./app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.notexpectedyet.com/NotExpectedYet/OctoFarm"
  },
  "bin": {
    "octofarm": "./app.js"
  },
  "keywords": [
    "octofarm",
    "octo",
    "octoprint",
    "pi",
    "node"
  ],
  "author": "James Mackay (NotExpectedYet)",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "express-ws": "^4.0.0",
    "flatted": "^2.0.2",
    "lodash": "^4.17.20",
    "mjpeg-decoder": "^1.0.3",
    "mongoose": "^5.10.7",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.2",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.1",
    "pm2": "^4.5.0",
    "request": "^2.88.2",
    "systeminformation": "^4.27.5",
    "wake_on_lan": "^1.0.0",
    "winston": "^3.3.3",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "clinic": "^6.0.3",
    "cssnano": "^4.1.10",
    "debug": "^4.2.0",
    "dependency-check": "^4.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-ejs-js": "^0.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-include": "^2.4.1",
    "gulp-rename": "^2.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^1.4.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.9",
    "prettier": "^2.1.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "pkg": {
    "assets": "views/**/*"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "debug": true,
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": "v12.19.0",
    "npm": ">= 6.0.0"
  },
  "config": {
    "imageRepo": "octofarm/octofarm",
    "imageName": "OctoFarm",
    "imagePort": "4000",
    "internalContainerPort": "4000"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "views/assets/*",
      "logs/*",
      "client_src/*"
    ],
    "delay": "2500"
  }
}
